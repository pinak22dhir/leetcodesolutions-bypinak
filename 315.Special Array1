class Solution {
    public boolean isArraySpecial(int[] nums) {
        for(int i=0;i<nums.length-1;i++){
            if(sameparity(nums[i],nums[i+1])){return false;}
        }
    return true; 
    }
    static boolean sameparity(int num1,int num2){
        return ((num1%2==0 && num2%2==0)||(num1%2!=0 && num2%2!=0));
    }
}
