class Solution {
    public int search(int[] nums, int target) {
      int l=0;
      int h=nums.length-1;
      while(l<=h){
        int mid=(l+h)/2;
        if(nums[mid]==target){
            return mid;
        }
        //left sorted
        else if(nums[l]<=nums[mid]){
            //l= 1          4
            if(target>=nums[l] && target<nums[mid]){
                 h=mid-1;
            }
            else{
                l=mid+1;
            }
        }
        //right sorted
        else if(nums[l]>nums[mid]){
            //l= 4  5 6 2 1
            if(target>nums[mid] && target<=nums[h]){
                 l=mid+1;
            }
            else{
               h=mid-1;
            }
        }
}
    return -1;
    }
}
