class Solution {
    public int[][] construct2DArray(int[] original, int m, int n) {
     int arr[][]=new int[m][n];
      int s=original.length;
      int pi=m*n;
      int k=0;
      System.out.println(s+" ");
      System.out.println(pi+" ");
      if(pi==s){
      for(int i=0;i<m;i++){
        for(int j=0;j<n;j++)
        {
            arr[i][j]=original[k++];
          
        }
      }
      }
      else{
        return new int[0][0];
      }
      return arr;
    }
}
